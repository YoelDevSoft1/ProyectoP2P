  "tags": ["p2p", "exchange", "trading"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "graphTooltip": 0,
  "time": {
    "from": "now-6h",
    "to": "now"
